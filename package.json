{
  "name": "aws-simple-example",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "scripts": {
    "bootstrap": "cdk bootstrap --app 'yarn aws-simple create' --profile clebert",
    "prebuild": "rm -rf dist",
    "build": "parcel build src/app/index.html --out-dir dist/app --public-url /assets",
    "postbuild": "tsc -P tsconfig.bff.json",
    "compile": "tsc --noEmit",
    "predeploy": "run-s build",
    "deploy": "cdk deploy --app 'yarn aws-simple create' --profile clebert --require-approval never",
    "postdeploy": "aws-simple upload --profile clebert",
    "format": "prettier --write '**/*.{js,json,md,ts,tsx,yml}'",
    "lint": "tslint --project . --format stylish",
    "prestart": "rm -rf dist",
    "start": "run-p start:*",
    "start:app-watcher": "parcel watch src/app/index.html --out-dir dist/app --public-url /assets",
    "start:bff-watcher": "tsc -P tsconfig.bff.json --watch",
    "start:server": "aws-simple start --port 1985 --cached"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.33",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "aws-cdk": "^1.12.0",
    "aws-simple": "^0.0.12",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "ts-config": "^20.10.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
